    "@nestjs/swagger": "^11.2.0",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.5.0",
    "joi": "^17.13.3",
    "lambda": "^0.10.3",
    "mongoose": "^8.14.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
